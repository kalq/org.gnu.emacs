[Application]
name=org.gnu.emacs
runtime=org.freedesktop.Sdk/x86_64/19.08
sdk=org.freedesktop.Sdk/x86_64/19.08
command=emacs

[Context]
shared=network;ipc;
sockets=x11;pulseaudio;
filesystems=/var/tmp;/tmp;host;

[Extension org.gnu.emacs.Debug]
directory=lib/debug
autodelete=true
no-autodownload=true

[Build]
built-extensions=org.gnu.emacs.Debug;